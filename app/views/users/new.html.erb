
<%= form_for(@user) do |f| %>
<ul>
  <%@user.errors.full_messages.each do |err| %>
  <li><%= err %></li>
  <% end %>
</ul>
<p>Your Username Here: </p><%= f.text_field :name %><br>
<p>Email: </p><%= f.email_field :email, placeholder: "example@example.com" %><br>
<p>Password: </p><%= f.password_field :password %>
<%= f.submit "Sign Up" %>
<% end %>